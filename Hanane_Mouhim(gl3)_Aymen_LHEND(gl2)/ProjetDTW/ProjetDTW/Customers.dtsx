                  Person.CountryRegion ON Person.StateProvince.CountryRegionCode = Person.CountryRegion.CountryRegionCode AND 
                  Person.StateProvince.CountryRegionCode = Person.CountryRegion.CountryRegionCode AND 
                  Person.StateProvince.CountryRegionCode = Person.CountryRegion.CountryRegionCode INNER JOIN
